{
  "name": "react-radio-group-context",
  "version": "1.1.2",
  "description": "Radio Group Component for React easy to configure and compose",
  "author": "EmaSuriano",
  "license": "MIT",
  "repository": "EmaSuriano/react-radio-group-context",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "prepare": "npm run build",
    "storybook": "start-storybook -p 9009",
    "deploy": "build-storybook && gh-pages -d storybook-static"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "devDependencies": {
    "@dump247/storybook-state": "^1.2.4",
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-centered": "^3.3.15",
    "@storybook/addon-info": "^3.3.15",
    "@storybook/addon-knobs": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "gh-pages": "^1.1.0",
    "jest": "^22.4.2",
    "node-sass": "^4.9.2",
    "react": "^16.3.0-alpha.1",
    "react-dom": "^16.3.0-alpha.1",
    "react-scripts": "^1.1.1",
    "react-test-renderer": "^16.2.0",
    "rollup": "^0.54.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-peer-deps-external": "^2.0.0",
    "rollup-plugin-postcss": "^1.1.0",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/test"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6"
  }
}
